stages:
  - deploy

deploy:
  image: $REGISTER/terraform:0.1.1-cielo
  stage: deploy
  script:
   - apk add curl
   - ls -l
   - curl https://terraform.cielocorp.cloud/.well-known/terraform.json
   - terraform --version
   - terraform init -backend-config backend_dev.hcl
   #- terraform init
   #- terraform import aws_dynamodb_table.autorizacaoRecarga autorizacaoRecarga
   - terraform apply --auto-approve
   #- terraform plan
  only:
   - develop